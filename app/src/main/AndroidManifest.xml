<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skwebs.auth">

<uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Auth">
        <activity
            android:name="com.skwebs.auth.UserActivity"
            android:exported="false" />
        <activity
            android:name="com.skwebs.auth.ForgetActivity"
            android:exported="false"
            android:parentActivityName="com.skwebs.auth.LoginActivity" />
        <activity
            android:name="com.skwebs.auth.RegisterActivity"
            android:parentActivityName="com.skwebs.auth.LoginActivity" />
        <activity
            android:name="com.skwebs.auth.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.skwebs.auth.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>